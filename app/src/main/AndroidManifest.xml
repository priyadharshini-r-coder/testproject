<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jetpackcompose">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JetpackCompose">
        <activity
            android:name=".ui.accounts.TeamGroupChatActivity"
            android:exported="false" />
        <activity
            android:name=".ui.accounts.ContactAdminActivity"
            android:exported="false" />
        <activity
            android:name=".ui.accounts.SystemALertActivity"
            android:exported="false" />
        <activity
            android:name=".ui.accounts.HandOffActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".ui.accounts.ResetSuccessActivity"
            android:exported="false" />
        <activity
            android:name=".ui.accounts.AppointmentSuccessActivity"
            android:exported="false" />
        <activity
            android:name=".ui.accounts.AddPatientActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".ComposeActivity"
            android:exported="true" />
        <activity
            android:name=".ui.accounts.FilterActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JetpackCompose.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>